{
    "name": "code-vgl",
    "displayName": "VGL Development Toolbox",
    "description": "Visual Studio Code extension for writing VGL code.",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.22.0"
    },
    "publisher": "IanCooper",
    "author": "Ian Cooper",
    "repository": "https://github.com/icooper/code-vgl",
    "license": "MIT",
    "contributes": {
        "languages": [{
            "id": "vgl-structure",
            "aliases": ["VGL Structure", "structure.txt"],
            "filenamePatterns": [
                "structure.txt",
                "structure.*.txt",
                "*.structure.txt",
                "structure.txt.*"
            ]
        }],
        "grammars": [{
            "language": "vgl-structure",
            "scopeName": "source.vgl-structure",
            "path": "./syntaxes/vgl-structure.json"
        }]
    }
}