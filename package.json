{
    "name": "code-vgl",
    "displayName": "VGL Development Toolbox",
    "description": "Visual Studio Code extension for writing VGL code.",
    "version": "0.0.6",
    "categories": [
        "Programming Languages"
    ],
    "engines": {
        "vscode": "^1.22.0"
    },
    "keywords": [
        "samplemanager",
        "lims",
        "vgl",
        "structure.txt"
    ],
    "qna": "https://github.com/icooper/code-vgl/issues",
    "publisher": "IanCooper",
    "author": {
        "name": "Ian Cooper",
        "url": "https://github.com/icooper"
    },
    "bugs": {
        "url": "https://github.com/icooper/code-vgl/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/icooper/code-vgl"
    },
    "homepage": "https://github.com/icooper/code-vgl",
    "license": "MIT",
    "contributes": {
        "languages": [
            {
                "id": "vgl-structure",
                "filenamePatterns": [
                    "structure.txt",
                    "structure.*.txt",
                    "*.structure.txt",
                    "structure.txt.*"
                ]
            },
            {
                "id": "vgl-report",
                "filenamePatterns": [
                    "*.rpf",
                    "*.sxf",
                    "*.caf"
                ]
            }
        ],
        "grammars": [
            {
                "language": "vgl-structure",
                "scopeName": "source.vgl-structure",
                "path": "./syntaxes/vgl-structure.json"
            },
            {
                "language": "vgl-report",
                "scopeName": "source.vgl-report",
                "path": "./syntaxes/vgl-report.json"
            }
        ]
    }
}